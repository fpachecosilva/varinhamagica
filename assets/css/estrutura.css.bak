	#ingressos .aviso { font-size: 13px; text-align: center; }
	
/* :Ingressos */

/* Icones: */
	#icones { padding: 0 10px; margin-bottom: 70px }
	#icones h1 { height: initial; font-size: 22px; line-height: 24px; width: 100%; height: 50px; margin-bottom: 7px }
	#icones h3 { color: #fff; font-size: 16px; margin-bottom: 30px }
	#icones h2 { font-size: 14px; line-height: 24px; font-weight: 400; margin-top: initial; text-align: center; width: 100%; max-width: 100%; }
	#icones .wrapper-card { text-align: left; height: auto; align-items: flex-start; }
	#icones p { float: left; font-size: 15px; margin-top: 5px; width: 215px; display: block; height: auto; }
	#icones .card { min-height: 90px; margin: 20px; }
