	#principal article img { padding: 0 20px; box-sizing: border-box; margin-top: 60px; }
	/* #principal h1 { font-size: 28px; line-height: 30px; height: initial; width: initial; padding-top: 40px; } */
	#principal h1 { font-size: 25px; line-height: 40px; height: initial; width: initial; }
	#principal h1 strong { font-size: 35px; display: block; margin: 18px 0; }
	#principal h3 { font-size: 16px; line-height: 30px; margin: 20px }
	/* :Principal */
	
	/* Menu Abas: */
	#menu-abas h2 { font-size: 25px; line-height: 30px; margin-top: 65px; }
	#menu-abas { max-width: 100%; margin-top: 30px; height: auto; }
	#menu-abas p { font-size: 16px; line-height: 26px; text-align: left; padding: 0 20px; }
	#exposicao a { font-size: 12px; }
	.menu-links { padding: 0; height: 30px }
	.menu-links a { font-size: 9px; width: 33.33%; height: 30px; line-height: 25px; float: left; }
	.menu-links .ativo { border-bottom: 3px solid #B6A168; font-size: 9px; width: 33.33% }
	.logo-parceiros { margin-top: 20px; max-width: 100%; margin-left: 0; padding: 0 20px; box-sizing: border-box; height: initial; }
	#megashow { margin-bottom: 60px!important; }
	.menu-locais-links a { font-size: 13px; flex: 1 0 calc(50% - 80px); height: 30px; padding: 15px 20px; }
/* :Menu Abas */

/* Ingressos: */
	#ingressos { width: initial; border-top: 0; padding: 0 20px; box-sizing: border-box; }
	#ingressos h1 { font-size: 22px; line-height: 30px; margin-top: 35px; margin-bottom: 20px; text-align: center; height: initial; }

	#ingressos .card { width: 100%; max-width: 50%; height: auto; margin: auto; display: flex; justify-content: center; flex: 1 0 50%; }

	#ingressos .card h2 { width: initial; font-size: 18px; line-height: 21px; margin: initial; text-align: center; height: initial; }
	#ingressos .card p { width: initial; font-size: 14px; line-height: 16px; margin: initial; text-align: center; height: initial; }
	#ingressos .card a { width: 70%; margin: initial; float: initial; font-size: 14px; line-height: 45px; padding: 10px; padding-top: 5px; box-sizing: border-box; min-height: 65px; margin-top: 15px; }	
	#ingressos .ingressos-mini-box { width: initial; padding: 20px; }
	#ingressos .ingressos-mini-box p { font-size: 10px; line-height: 13px }
	#ingressos .ingressos-mini-box-info, #ingressos .ingressos-mini-box-info.first { width: initial; }
	#ingressos .ingressos-mini-box-info a { font-size: 14px; line-height: 50px; padding: 0 20px; width: 100%; line-height: 15px; font-weight: 400; height: 50px; padding-top: 10px; padding-right: 70px }
	#ingressos .ingressos-mini-box-info.first a { padding-top: 17px }
	#ingressos .ingressostxt p { margin-top: 0; }
	.seta { float: right; margin-top: -33px; margin-right: 20px; -webkit-transition: all var(--tempo); -moz-transition: all var(--tempo); -ms-transition: all var(--tempo); -o-transition: all var(--tempo); transition: all var(--tempo); }
	.setacima { -ms-transform: rotate(180deg); /* IE 9 */ -webkit-transform: rotate(180deg); /* Safari 3-8 */ transform: rotate(180deg); }
	.setabaixo { -ms-transform: rotate(0deg); /* IE 9 */ -webkit-transform: rotate(0deg); /* Safari 3-8 */ transform: rotate(0deg); }

	#ingressos .wrapper-card { grid-template-columns: initial; margin-bottom: initial; }
	/* #ingressos .wrapper-card:nth-of-type(2) { grid-template-columns: initial; } */
