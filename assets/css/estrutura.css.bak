	#icones .card .img-wrapper { text-align: center; min-height: 100px; }
	#icones .card img { max-height: 55px }
	#icones .box-icones { margin-top: 50px }
	#icones .box-icones p { font-size: 12px; width: initial; height: initial; line-height: 16px; text-align: left; padding: 17px }
/* :Icones */

/* Valores: */
	#valores { margin-top: 30px; padding: 0 20px; }
	#valores h1 { font-size: 22px; line-height: 24px; margin: initial; }
	#valores h2 { font-size: 18px; line-height: 24px; margin: auto; margin-top: 5px;  }
	#valores table { max-width: 100%; margin: auto; width: initial; height: initial; margin-top: 20px; margin-bottom: 20px }
	#valores table td { width: 25%; height: 28px; font-size: 12px; padding: 5px; }

	.menu-locais-links:after { width: 70%; }
/* :Valores */

	footer article { padding: 0 }
	footer .footer-card { width: 100%; float: initial; margin: initial; margin: 20px 0 }
	footer .footer-card p { font-size: 16px; line-height: 21px }
	footer .footer-card a { width: 150px; height: 45px; float: initial; font-size: 14px; margin: 20px auto; line-height: 45px }
	footer img:nth-child(6), footer img:nth-child(7), footer img:nth-child(8), footer img:nth-child(9) { padding-top: 0; }
	footer .parceiros { font-size: 14px; padding-bottom: 30px; }
	footer .marcasDesk { max-width: 100%; }    	
}


