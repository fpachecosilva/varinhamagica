	footer .marcasDesk { display: block !important; }

/* Elementos Desktop: */
	h1 {}
	h2 {}
	h3 {}
	h4 {}
	h5 {}
	article {padding: 50px; max-width: 1440px; margin: 0 auto;}
/* :Elementos Desktop */

/* Header Desktop: */
	header {}
/* :Header Desktop */

/* Footer Desktop: */
	footer {}
	#Copyright {}
/* :Footer Desktop */

/* Página Home Desktop: */
