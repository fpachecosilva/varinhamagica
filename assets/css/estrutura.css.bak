	img { max-width: 100% }
	.bg { max-width: initial; }
	article { padding: 15px }
	.Btn-primary { margin-right: 0; width: 220px; margin-top: 25px }
	.Btn-secondary { background: none; border: none; margin-top: 15px; text-decoration: underline; }
/* :Página Home Mobile */

/* Principal: */
