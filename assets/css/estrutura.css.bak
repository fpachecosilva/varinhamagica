/* -------------------------------- Versionamento Mobile: --------------------------------- */	

@media screen and (max-width: 900px) { 
	.DesktopItem {display: none !important;}
	.MobileItem {display: inline-block !important;}
		
/* Elementos Mobile: */
	h1 {}
	h2 {}
	h3 {}
	h4 {}
	h5 {}
	article {padding: 20px;}
/* :Elementos Mobile */

/* Header Mobile: */
	header {}
/* :Header Mobile */

/* Footer Mobile: */
	footer { padding: 0 20px; box-sizing: border-box; text-align: center; height: initial; }

	#Copyright {}
/* :Footer Mobile */

/* Página Home Mobile: */
